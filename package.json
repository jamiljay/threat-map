{
  "name": "cyber-attacks",
  "version": "0.0.1",
  "description": "Save a server, save the world",
  "main": "main.js",
  "author": "Jamil Shehadeh",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/jamiljay/threat-map"
  },
  "url": "https://github.com/jamiljay/threat-map",
  "email": "jamiljaymail@yahoo.com",
  "scripts": {
    "start": "cross-env DEV_SERVER=true webpack-dev-server",
    "server": "cross-env NODE_ENV=development nodemon --watch ./server ./server/server.js",
    "dev": "webpack --watch",
    "build:prod": "cross-env NODE_ENV=production webpack -p",
    "nyc": "nyc ava",
    "ava": "ava"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{scss,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "babel": {
    "presets": [
      "env",
      "react",
      "stage-2"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "ava": {
    "files": [
      "client/**/*_test.js"
    ],
    "source": [
      "dist/*"
    ],
    "timeout": "2m",
    "concurrency": 10,
    "failFast": false,
    "failWithoutAssertions": true,
    "require": [
      "./helpers/testing/setup-browser-env.js",
      "./helpers/testing/handsontable-polyfill.js",
      "./helpers/testing/jquery-polyfill.js",
      "babel-register",
      "ignore-styles"
    ],
    "babel": "inherit"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.1.1",
    "chance": "^1.0.16",
    "express": "^4.16.3",
    "font-awesome": "^4.7.0",
    "immutable": "^3.8.1",
    "prop-types": "^15.6.2",
    "react": "^16.2.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-simple-maps": "^0.12.1",
    "reactstrap": "^6.1.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.3.0",
    "redux-tooltip": "^0.7.2",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1",
    "topojson-client": "^3.0.0"
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "browser-env": "^3.2.4",
    "clean-webpack-plugin": "^0.1.18",
    "copy-webpack-plugin": "^4.5.1",
    "core-js": "^2.5.4",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.1",
    "enzyme": "^3.3.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "expose-loader": "^0.7.3",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^3.2.0",
    "ignore-styles": "^5.0.1",
    "inspect-process": "^0.5.0",
    "json-loader": "^0.5.7",
    "lint-staged": "^7.0.5",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.0.0",
    "node-watch": "^0.5.5",
    "nodemon": "^1.17.5",
    "nyc": "^11.3.0",
    "prettier-eslint": "^8.8.1",
    "prettier-eslint-cli": "^4.7.1",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "^4.2.0",
    "redux-devtools": "^3.3.2",
    "redux-mock-store": "^1.5.1",
    "sass-loader": "^6.0.6",
    "sinon": "^5.1.0",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^4.10.2",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-cli": "^2.1.4",
    "webpack-dev-server": "^3.1.4"
  }
}
